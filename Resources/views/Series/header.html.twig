<p class="titulo_widget titulo_widget_grande" style="margin-left: 0;">
  {{ series.getTitle()|default("No title"|trans) }}
</p>
<div class="cab_serial">
  {% if series.getSubtitle() is not empty %}
  <h5 class="subtitle">
    {{ series.getSubtitle() }}
  </h5>
  {% endif %}
  {% if series_precinct %}
    {% if series_precinct.getCod() != "PRECINCT1" %}
    <h3 class="place">
      {{ precinct_complete_name(series_precinct, app.request.getLocale()) }}
    </h3>
    {% endif %}
  {% endif %}
</div>
